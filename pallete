SYMBOL=${1:-####}

for i in {0..15}; do
    echo -en "\e[$(($i/8));$((30+$i%8))m$SYMBOL "
done; echo -e "\e[m"
echo

for i in {0..15}; do
    echo -en "\e[38;5;$(($i))m$SYMBOL "
done; echo -e "\e[m"
echo

for i in {0..5}; do
    for j in {0..35}; do
        echo -en "\e[38;5;$((16+$i*36+j))m$SYMBOL "
    done
    echo -e "\e[m"
done
echo 

for i in {0..23}; do
    echo -en "\e[38;5;$((232+$i))m$SYMBOL "
done; echo -e "\e[m"
